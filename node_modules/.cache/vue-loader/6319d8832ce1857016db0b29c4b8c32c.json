ue\" placeholder=\"Select\">\r\n                    <el-option\r\n                        v-for=\"item in selectData\"\r\n                        :key=\"item.id\"\r\n                        :label=\"item.value\"\r\n                        :value=\"item.id\"\r\n                    />\r\n                </el-select>\r\n            </el-form-item>\r\n            <el-form-item>\r\n                <el-button type=\"primary\" @click=\"onSubmit\">Query</el-button>\r\n                <el-button type=\"success\" @click=\"addData\">Add</el-button>\r\n                <el-button type=\"info\">ExportExecl</el-button>\r\n                <el-button type=\"warning\">ImportExecl</el-button>\r\n            </el-form-item>\r\n        </el-form>\r\n        <el-table :data=\"tableData\" style=\"width: 100%\">\r\n            <el-table-column type=\"selection\" width=\"55\" />\r\n            <el-table-column prop=\"studyInfoName\" label=\"StudyInfoName\" />\r\n            <el-table-column prop=\"studyInfoDescribe\" label=\"StudyInfoDescribe\" />\r\n            <el-table-column prop=\"studyTypeName\" label=\"StudyTypeName\" />\r\n            <el-table-column prop=\"createTime\" label=\"CreateTime\" />\r\n            <el-table-column prop=\"updateTime\" label=\"UpdateTime\" />\r\n            <el-table-column label=\"Operations\">\r\n                <template #default=\"scope\">\r\n                    <el-button size=\"small\" @click=\"handleEdit(scope.$index, scope.row)\">Edit</el-button>\r\n                    <el-button\r\n                        size=\"small\"\r\n                        type=\"danger\"\r\n                        @click=\"handleDelete(scope.$index, scope.row)\"\r\n                    >Delete</el-button>\r\n                </template>\r\n            </el-table-column>\r\n        </el-table>\r\n        <el-pagination small background layout=\"prev, pager, next\" :total=\"50\" class=\"mt-4\"></el-pagination>\r\n        <el-dialog v-model=\"centerDialogVisible\" :title=\"tip\" width=\"30%\" center>\r\n            <el-form :inline=\"true\" :model=\"fromData\" class=\"demo-form-inline\">\r\n                <el-form-item label=\"StudyInfoName\">\r\n                    <el-input\r\n                        v-model=\"fromData.studyInfoName\"\r\n                        placeholder=\"Please Input StudyInfoName\"\r\n                    />\r\n                </el-form-item>\r\n                <el-form-item label=\"StudyTypeName\">\r\n                    <el-select\r\n                        v-model=\"fromData.studyTypeId\"\r\n                        :clearable=\"true\"\r\n                        placeholder=\"Select\"\r\n                    >\r\n                        <el-option\r\n                            v-for=\"item in selectData\"\r\n                            :key=\"item.id\"\r\n                            :label=\"item.value\"\r\n                            :value=\"item.id\"\r\n                        />\r\n                    </el-select>\r\n                </el-form-item>\r\n                <el-form-item label=\"StudyInfoDescribe\">\r\n                    <el-input\r\n                        v-model=\"fromData.studyInfoDescribe\"\r\n                        placeholder=\"Please Input StudyInfoDescribe\"\r\n                    />\r\n                </el-form-item>\r\n                <template #footer>\r\n                    <span class=\"dialog-footer\">\r\n                        <el-button @click=\"centerDialogVisible = false\">Cancel</el-button>\r\n                        <el-button type=\"primary\" @click=\"centerDialogVisible = false\">Confirm</el-button>\r\n                    </span>\r\n                </template>\r\n            </el-form>\r\n        </el-dialog>\r\n    </span>\r\n</template>\r\n\r\n<script lang=\"ts\" setup>\r\nimport { Search } from '@element-plus/icons-vue'\r\nimport { reactive, ref } from 'vue'\r\nimport StudyInfoApi from '@/api/studyInfoApi'\r\n//控制模态框开关\r\nconst centerDialogVisible = ref(false)\r\n//模态框提示\r\nconst tip = ref(\"tip\")\r\n//表格数据\r\nconst tableData: any = ref([])\r\n//下拉框数据绑定\r\nconst selectData: any = ref([]);\r\n//下拉框数据获取，并存入浏览器数据库\r\nif (window.localStorage.getItem(\"SelectData\") != null) {\r\n    selectData.value = JSON.parse(window.localStorage.getItem(\"SelectData\") ?? '')\r\n} else {\r\n    StudyInfoApi.GetStudyTypeAsync().then(result => {\r\n        selectData.value = result.data\r\n        window.localStorage.setItem(\"SelectData\", JSON.stringify(result.data))\r\n        console.log(selectData)\r\n    })\r\n}\r\n\r\nconst formInline = reactive({\r\n    studyInfoName: '',\r\n    studyTypeId: ''\r\n})\r\n//表格数据获取\r\nconst loadTableData = () => {\r\n    console.log(formInline)\r\n    StudyInfoApi.GetStudyInfoViewAsync(formInline.studyInfoName, formInline.studyTypeId).then(result => {\r\n        tableData.value = result.data\r\n        console.log(result)\r\n    })\r\n}\r\nloadTableData()\r\n//表格数据查询\r\nconst onSubmit = () => {\r\n    loadTableData()\r\n    console.log('select')\r\n}\r\ninterface StudyInfo {\r\n    id: string;\r\n    studyInfoName: string;\r\n    studyInfoDescribe: string;\r\n    studyTypeId: string;\r\n}\r\nconst fromData: StudyInfo = reactive({\r\n    id: '',\r\n    studyInfoName: '',\r\n    studyInfoDescribe: '',\r\n    studyTypeId: ''\r\n})\r\n//修改\r\nconst handleEdit = (index: number, row: any) => {\r\n    console.log(index, row)\r\n}\r\n//删除\r\nconst handleDelete = (index: number, row: any) => {\r\n    console.log(index, row)\r\n}\r\n//添加\r\nconst addData = () => {\r\n    centerDialogVisible.value = true\r\n    tip.value = '新增数据'\r\n}\r\n</script>\r\n<style>\r\n.el-pagination {\r\n    float: right;\r\n    margin-top: 10px;\r\n}\r\n</style>"]}]}